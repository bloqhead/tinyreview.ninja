---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - review
    - reviews
    - tagList
  addAllPagesToCollections: true
layout: layouts/main.njk
eleventyComputed:
  title: Tagged "{{ tag }}"
permalink: /tags/{{ tag }}/
---
<h1>Tagged &quot;{{ tag }}&quot;</h1>

{% set postslist = collections[ tag ] %}
{% include 'partials/postlist.njk' %}

<p>See <a href='{{ '/tags/' | url }}'>all tags</a>.</p>